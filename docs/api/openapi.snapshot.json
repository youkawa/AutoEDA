            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RecipeEmitRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RecipeEmitResult"
                }
              }
            }
          },
          "422": {
